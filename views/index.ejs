<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
  </head>
  <body>
    <div>
      <a href="/create">create</a>
    </div>
    <div>
      <table>
        <tr>
          <th>id</th>
          <th>name</th>
          <th>url</th>
          <th>storeId</th>
          <th>price</th>
          <th>tbnailImg</th>
          <th>delete</th>
        </tr>
        <tr>
          <td>0</td>
          <td>productZero</td>
          <td>https://www.yeslkoh.com</td>
          <td>1</td>
          <td>set other price too</td>
          <td>tbnail img src</td>
          <td><button class="del"> X </button></td>
        </tr>
        <%dealData.forEach(function (item, idx) {%>
          <tr>
            <td><%=item.id%></td>
            <td><%=item.name%></td>
            <td><%=item.url%></td>
            <td>1</td>
            <td><%=item.price.nPrice%></td>
            <td><img src="<%=item.img.tbnail.src%>" alt="item.img.tbnail.name%>" width="100" height="70"/></td>
            <td>
              <form action="/del" method="post">
                <button type="submit" value="<%=item.id%>" name="id"> X </button>
              </form>
            </td>
          </tr>
        <%});%>
      </table>
    </div>
  </body>
</html>
